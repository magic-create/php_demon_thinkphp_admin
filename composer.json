{
    "name": "comingdemon/admin-thinkphp",
    "type": "library",
    "description": "Universal PHP admin quick development library for thinkphp",
    "homepage": "http://www.m-create.cn",
    "keywords": [
        "comingdemon",
        "thinkphp-admin"
    ],
    "authors": [
        {
            "name": "ComingDemon"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2.0",
        "topthink/framework": ">=6.0",
        "comingdemon/demon": "^1.0",
        "gregwar/captcha": "^1.1.9",
        "phpoffice/phpspreadsheet": "^1.16"
    },
    "autoload": {
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "Demon\\AdminThinkPHP\\": "src"
        }
    },
    "extra": {
        "think": {
            "services": [
                "Demon\\AdminThinkPHP\\AdminService"
            ]
        }
    }
}
